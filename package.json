{
  "name": "@euphrasiologist/lwphylo",
  "version": "1.2.2",
  "description": "A lightweight, low level javascript library to plot phylogenies from Newick files.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./draw": {
      "import": "./dist/drawPhylogeny.esm.js",
      "require": "./dist/drawPhylogeny.cjs",
      "default": "./dist/drawPhylogeny.umd.js"
    }
  },
  "browser": "dist/drawPhylogeny.umd.js",
  "unpkg": "dist/drawPhylogeny.umd.js",
  "jsdelivr": "dist/drawPhylogeny.umd.js",
  "sideEffects": false,
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src",
    "prepublishOnly": "rm -rf dist && npm run build && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Euphrasiologist/lwPhylo.git"
  },
  "keywords": [
    "phylogeny",
    "visualisation",
    "newick",
    "d3",
    "phylogenetics"
  ],
  "author": "Max Brown",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Euphrasiologist/lwPhylo/issues"
  },
  "homepage": "https://github.com/Euphrasiologist/lwPhylo#readme",
  "peerDependencies": {
    "d3": "^7 || ^8"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "jsdom": "27",
    "rollup": "^4.52.4"
  },
  "engines": {
    "node": ">=18"
  }
}
